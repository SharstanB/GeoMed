<script>
    $(document).ready(function () {
        $("#m_ver_menu li").on("click", function (e) {
            if ($(this).parent().hasClass('m-menu__subnav')) {
                $(this).parent().parent().addClass("m-menu__item--active");
            } else {
                $("#m_ver_menu li").removeClass("m-menu__item--active");
                $(this).addClass("m-menu__item--active");
            }
        });
    });
</script>


